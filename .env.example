# Shared Database (used by multiple packages)
POSTGRES_URL=postgres://user:password@host:port/database

# Shared Redis/Caching (used by multiple apps)
KV_REST_API_URL=https://your-upstash-redis-url.upstash.io
KV_REST_API_TOKEN=your_upstash_token
# OR use standard Redis:
REDIS_URL=redis://user:password@host:port

# Shared Neynar Configuration (used by multiple apps)
NEYNAR_API_KEY=your_neynar_api_key
NEYNAR_CLIENT_ID=your_neynar_client_id

# Shared Blockchain Configuration
ALCHEMY_API_KEY=your_alchemy_api_key
# RPC_URL is used by server-side code
RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_API_KEY
# NEXT_PUBLIC_RPC_URL is used by client-side code (must be prefixed with NEXT_PUBLIC_ to be accessible in the browser)
# Set this to the same value as RPC_URL to ensure consistent RPC endpoint usage
NEXT_PUBLIC_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_API_KEY
